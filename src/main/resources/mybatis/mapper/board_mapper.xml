<mapper namespace="com.heatokpia.mapper.BoardMapper">
	<select id="findById" parameterType="String" resultType="Board">
	 <![CDATA[
		]]>
	</select>
	
	<insert id="save" parameterType="com.heatokpia.dto.BoardNonMember">
